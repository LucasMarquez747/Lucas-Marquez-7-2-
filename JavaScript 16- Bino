let numeros = prompt("CADENA DE NUMEROS");
let multiplos= [2,3,4,5];
let sumContadorMultiplo2 = 0;
let sumContadorMultiplo3 = 0;
let sumContadorMultiplo4 = 0;
let sumContadorMultiplo5 = 0;

    let arrayNumeros = numeros.split(" ")
    console.log(arrayNumeros)
for (let i = 0; i < arrayNumeros.length; i++) {
    for (let j = 0; j < multiplos.length; j++) {
        
        if ( arrayNumeros[i] % multiplos[j] ==0) {
            switch (multiplos[j]) {
                case 2:
                    sumContadorMultiplo2++
                    break;
            
                case 3:
                        sumContadorMultiplo3++
                        break;
                case 4:   
                       sumContadorMultiplo4++;
                    break;
                case 5:
                       sumContadorMultiplo5++;
                        break;

c            }
            


}
}
}

document.write("se encontro "+ sumContadorMultiplo2+" multiplos de 2 <br>")
document.write("se encontro "+ sumContadorMultiplo3+" multiplos de 3 <br>")

document.write("se encontro "+ sumContadorMultiplo4+" multiplos de 4 <br>")
document.write("se encontro "+ sumContadorMultiplo5+" multiplos de 5<br>")




